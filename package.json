{
  "name": "@adr-express-ts/core",
  "version": "2.2.2",
  "description": "This package is a dependency injection for express with typescript using decorators and the Action–Domain–Responder pattern",
  "main": "lib/index.js",
  "types": "lib",
  "files": [
    "lib/*"
  ],
  "keywords": [
    "Express",
    "express",
    "typescript",
    "TypeScript",
    "Decorator",
    "Decorators",
    "decorator",
    "decorators",
    "controller",
    "Controller",
    "action",
    "domain",
    "responder",
    "Action",
    "Domain",
    "Responder",
    "Inject",
    "inject",
    "ES2017",
    "ES6",
    "ES5",
    "back-end",
    "server",
    "rest",
    "api",
    "mvc",
    "MVC",
    "adr",
    "ADR"
  ],
  "scripts": {
    "generate:docs": "rm -rf ./docs/ && yarn build && node_modules/.bin/jsdoc --configure .jsdoc.json --verbose && node ./scripts/build-docs.js",
    "build": "yarn lint && rm -rf ./lib/ && tsc -p .",
    "test": "yarn build && node ./scripts/setup-tests.js",
    "lint": "eslint src/**/*.ts",
    "read:docs": "serve ./docs",
    "version": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaresAil/adr-express-ts.git"
  },
  "engines": {
    "node": ">=12"
  },
  "author": "Nicolae-Rares Ailincai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RaresAil/adr-express-ts/issues"
  },
  "homepage": "https://github.com/RaresAil/adr-express-ts#readme",
  "devDependencies": {
    "@types/async-lock": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/object-hash": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.31.2",
    "braintree-jsdoc-template": "^3.3.0",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-security": "^1.4.0",
    "fs-extra": "^10.0.0",
    "jsdoc": "^3.6.7",
    "jsdoc-escape-at": "^1.0.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "async-lock": "^1.3.0",
    "express": "^4.17.1",
    "object-hash": "^2.2.0",
    "re2": "^1.16.0"
  }
}
