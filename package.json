{
  "name": "@adr-express-ts/core",
  "version": "2.0.0",
  "description": "This package is a dependency injection for express with typescript using decorators and the Action–Domain–Responder pattern",
  "main": "lib/index.js",
  "types": "lib",
  "files": [
    "lib/*"
  ],
  "keywords": [
    "Express",
    "express",
    "typescript",
    "TypeScript",
    "Decorator",
    "Decorators",
    "decorator",
    "decorators",
    "controller",
    "Controller",
    "action",
    "domain",
    "responder",
    "Action",
    "Domain",
    "Responder",
    "Inject",
    "inject",
    "ES2017",
    "ES6",
    "ES5",
    "back-end",
    "server",
    "rest",
    "api",
    "mvc",
    "MVC",
    "adr",
    "ADR"
  ],
  "scripts": {
    "generate:docs": "rm -rf ./docs/ && yarn build && node_modules/.bin/jsdoc --configure .jsdoc.json --verbose && node ./scripts/build-docs.js",
    "build": "yarn lint && rm -rf ./lib/ && tsc -p .",
    "test": "yarn build && node ./scripts/setup-tests.js",
    "lint": "eslint src/**/*.ts",
    "read:docs": "serve ./docs",
    "version": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn generate:docs",
      "pre-push": "yarn test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaresAil/adr-express-ts.git"
  },
  "author": "Nicolae-Rares Ailincai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RaresAil/adr-express-ts/issues"
  },
  "homepage": "https://github.com/RaresAil/adr-express-ts#readme",
  "devDependencies": {
    "@types/async-lock": "^1.1.2",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/object-hash": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "braintree-jsdoc-template": "^3.3.0",
    "eslint": "^7.24.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-security": "^1.4.0",
    "fs-extra": "^9.1.0",
    "husky": "^6.0.0",
    "jsdoc": "^3.6.6",
    "jsdoc-escape-at": "^1.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "async-lock": "^1.2.8",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "object-hash": "^2.1.1",
    "re2": "^1.15.9"
  }
}
